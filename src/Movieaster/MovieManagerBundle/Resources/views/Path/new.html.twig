{% extends '::base.html.twig' %}

{% block body %}

<h1>Path creation</h1>

<form action="{{ path('path_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit" class="btn btn-primary">Create</button>
    </p>
</form>

<ul>
    <li>
        <a href="{{ path('path') }}">
            Back to the list
        </a>
    </li>
</ul>

{% endblock %}